<%= form_for [:admin, @product] do |f| %>

<div class="group">
<%= f.label("title") %>
<%= f.text_field :title %>	
</div>

<div class="group">
<%= f.label("description") %>
<%= f.text_area :description %>	
</div>

<div class="group">
<%= f.label("quantity") %>
<%= f.text_field :quantity %>	
</div>

<div class="group">
<%= f.label("price") %>
<%= f.text_field :price %>	
</div>

<%= f.submit "Submit", data:{ disable_with: "Submitting..."} %>

<% end %>